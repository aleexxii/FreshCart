										<% topSellingCategories.forEach(category => { %>
											<% for( let i = 1; i <=topSellingCategories.length ; i++ ) { %>
                                       <tr>
										   <td>#<%- i %></td>
                                          <td><%- category.categoryDetails.categoryName %></td>
                                          <td><%- category.categoryDetails.discount %></td>
										  <% let revenue = category.totalPrice.toFixed(2) %>
                                          <td><%- revenue %></td>
										</tr>
										<% } %>
										<% }) %>
                                    </tbody>
                                 </table>
                              </div>
                           </div>
                        </div>
