					<% 
					let startPage = Math.max( 1 , currentPage - 1 );		// start page
					let endPage = Math.min( startPage + 2 , totalPages )		//end page
					%>
                      <% for( let i = startPage; i < endPage; i++ ) { %>
                      <li class="page-item">
                        <a
                          class="page-link<%= i === currentPage ? ' active' : '' %>"
                          href="./products?page=<%- i %>"
                          ><%- i %></a
                        >
                      </li>
                      <% } %>

                      <!-- Next Button -->
                      <% if (next) { %>
                      <li class="page-item">
                        <a
                          class="page-link"
                          href="./products?page=<%- next %>"
                          >Next</a
                        >
                      </li>
                      <% } %>
                    </ul>
                  </nav>
                </div>
              </div>
            </div>
          </div>
        </div>
      </main>
    </div>

    <!-- Libs JS -->
    <script src="../libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="../libs/simplebar/dist/simplebar.min.js"></script>

    <!-- Theme JS -->
    <script src="../js/theme.min.js"></script>
  </body>
</html>
